void dfs()
{
    stack<int> a;
    a.push(start);
    while (!a.empty())
    {
        int v = s.top();
        s.pop();
        for (int i = 0; i < ver[v].size(); ++i)
        {
            if (mark[ver[v][i]] == 0)
            {
                s.push(ver[v][i]);
                mark[ver[v][i]] = 1;
            }
        }
    }
}
